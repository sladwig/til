proc f (x y z) {
    print "x is $x"
    set x [g $x]
    print "g(x) becomes $x"
    print "y is $y"
    print "z is $z"
    return $x $y $z
}

proc g (a) {
    return ">$a<"
}

print [g "blebs"]
print [f 100 200 300]

set result [f 1 2 3] [g 4]
set expected ">1<23>4<"
print "$result == $expected?"
assert ("$result" == $expected)
print " $result == $expected, indeed"
