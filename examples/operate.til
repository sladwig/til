print "START"

# NameAtom:
assert (a == a)
print "a == a"

type number {
    proc init (x) {
        print "init: $x"
        return $x
    }
    proc "+" (a b) {
        return 0
    }
}

set n [number 5]
assert ((1 - $n) == -4)
print $(1 + $n)
assert ((1 + $n) == 6)
assert (($n + 1) == 0)

print "END"
