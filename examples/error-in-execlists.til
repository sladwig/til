print "START"

proc error_test () {
    proc on.error (e) {
        print "on.error: $e"
        set msg <$e message>
        print " message: $msg"
        return "something else"
    }

    set x [invalidcommand 1 2 3]
    print <$x invalidextraction>
    return "happy path that shouldn't happen"
}

set result [error_test]
assert ($result == "something else")

print "END"
